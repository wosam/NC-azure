    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11",
    "regenerator-runtime": "latest",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
